# Lab
Host corelab1
     HostName corelab1.mcb.uconn.edu
     IdentityFile ~/.ssh/id_rsa-corelab1
     ProxyJump becat

Host core
     HostName corelab2.mcb.uconn.edu
     User pan14001
     IdentityFile ~/.ssh/id_rsa-corelab2-pan14001
     ProxyJump becat

Host core-root
     HostName corelab2.mcb.uconn.edu
     User root
     IdentityFile ~/.ssh/id_rsa-corelab2-root
     ProxyJump becat

# HPC
Host becat
     User pan14001
     HostName becat-pan.engr.uconn.edu
     IdentityFile ~/.ssh/id_rsa-becat

Host cheyenne
     User pnanda
     HostName cheyenne.ucar.edu

Host xanadu
     HostName xanadu-submit-int.cam.uchc.edu
     User pnanda
     IdentityFile ~/.ssh/id_rsa-uchc
     ProxyJump slurm18-hn

Host hpc1
     HostName login1.storrs.hpc.uconn.edu
     User pan14001
     IdentityFile ~/.ssh/id_rsa-hpc-pan14001
     ProxyJump becat

Host admin1
     User pan14001admin
     HostName login1.storrs.hpc.uconn.edu
     IdentityFile ~/.ssh/id_rsa-hpc-pan14001admin
     ProxyJump becat

# Proxies
Host slurm18-hn
     HostName slurm18-hn.cam.uchc.edu
     User pnanda
     IdentityFile ~/.ssh/id_rsa-uchc
     ProxyJump xanadu-ext

Host xanadu-ext
     HostName xanadu-submit-ext.cam.uchc.edu
     User pnanda
     IdentityFile ~/.ssh/id_rsa-uchc

# Identities
Host gitlab.com
     HostName gitlab.com
     IdentityFile ~/.ssh/id_rsa-gitlab
     IdentitiesOnly yes

Host github.com
     HostName github.com
     IdentityFile ~/.ssh/id_rsa-github
     IdentitiesOnly yes

Host github.uconn.edu
     HostName github.uconn.edu
     User git
     IdentityFile ~/.ssh/id_rsa-github_uconn
     IdentitiesOnly yes
